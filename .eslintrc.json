{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["prettier"],
    "ignorePatterns": ["webpack.config.js"],
    "rules": {
      "prettier/prettier": ["error", {"endOfLine": "auto"}],
      "no-undef": "off",
      "no-unused-vars": "off"
    }
  }
